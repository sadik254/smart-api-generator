{
  "name": "saleh/smart-api-generator",
  "description": "Laravel package to auto-generate API scaffolding (model, migration, controller, routes)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "MD.Saleh Sadik",
      "email": "sadik254@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Saleh\\SmartApiGenerator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
         "Saleh\\SmartApiGenerator\\SmartApiServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": [
    "laravel",
    "api",
    "generator",
    "scaffold",
    "artisan",
    "crud",
    "smart-api"
  ],
  "homepage": "https://github.com/sadik254/smart-api-generator",
  "support": {
    "issues": "https://github.com/sadik254/smart-api-generator/issues",
    "source": "https://github.com/sadik254/smart-api-generator"
  }
}
